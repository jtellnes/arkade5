  <ItemGroup>
    <None Update="Bundled\Siegfried\default.sig">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Bundled\Siegfried\LICENSE.txt">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
    <None Update="Bundled\Siegfried\siegfried_linux">
