    </EmbeddedResource>
    <EmbeddedResource Update="Resources\OutputNames.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>OutputNames.Designer.cs</LastGenOutput>
      <CustomToolNamespace></CustomToolNamespace>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Report.nb-NO.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
    </EmbeddedResource>
    <EmbeddedResource Update="Resources\Report.resx">
      <Generator>PublicResXFileCodeGenerator</Generator>
      <LastGenOutput>Report.Designer.cs</LastGenOutput>
      <CustomToolNamespace></CustomToolNamespace>
    </EmbeddedResource>
